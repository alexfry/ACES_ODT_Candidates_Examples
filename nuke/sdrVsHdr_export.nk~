  DRT_ZCAM_v12_Kernel_w1 {{(0.595*DRT_ZCAM_v12_Kernel_Lp/10000)**0.931+1.037}}
  DRT_ZCAM_v12_Kernel_s1 {{DRT_ZCAM_v12_Kernel_w1*DRT_ZCAM_v12_Kernel_Lp/100}}
  DRT_ZCAM_v12_Kernel_ex -0.26
